algoritmo "Enquanto - Número menor que 10"
// Função :
// Autor : Isabela
// Data : 09/07/2024
// Seção de Declarações 
var

   numero : inteiro

inicio

      numero <- 1

      Enquanto numero < 10 faca
      
               Escreval(numero)
               numero <- numero + 1
               
      FimEnquanto

fimalgoritmo
