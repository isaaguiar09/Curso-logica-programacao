algoritmo "semnome"
// Função :
// Autor :
// Data : 11/07/2024
// Seção de Declarações 
var

   numero, par : inteiro

inicio

      numero <- 1
      
      Repita
      
      numero <- numero + 1
      
      Se ( numero mod 2 = 0) Entao
      
            par <- numero
            Escreval(par)
      
      FimSe
      
      ate numero = 100

fimalgoritmo
